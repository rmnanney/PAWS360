/var/log/adminlte/data-service/*.log {
    daily
    missingok
    rotate 30
    compress
    delaycompress
    notifempty
    create 0644 {{ adminlte_user | default('adminlte') }} {{ adminlte_user | default('adminlte') }}
    postrotate
        systemctl reload adminlte-data-service
    endscript
}