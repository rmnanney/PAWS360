    {
      "name": "5) Advising",
      "item": [
        {
          "name": "Resolve Advisor Id",
          "request": {
            "method": "GET",
            "url": { "raw": "{{baseUrl}}/advising/advisors", "host": ["{{baseUrl}}"], "path": ["advising", "advisors"] }
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "const list = pm.response.json();",
                  "const email = pm.environment.get('advisorEmail');",
                  "const found = (list || []).find(a => a.email === email);",
                  "if (found) { pm.environment.set('advisorId', found.advisorId); }",
                  "else { pm.test('advisor present', () => pm.expect(found).to.exist); }"
                ]
              }
            }
          ]
        },
        {
          "name": "Assign Primary Advisor",
          "request": {
            "method": "POST",
            "header": [
              { "key": "Content-Type", "value": "application/json" }
            ],
