{
  "name": "nextn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest",
    "test:integration": "./mvnw test -Dtest=T057IntegrationTest",
    "test:e2e": "cd tests/ui && npm test",
    "test:e2e:headed": "cd tests/ui && npm run test:headed",
    "test:e2e:debug": "cd tests/ui && npm run test:debug",
    "test:performance": "cd tests/performance && ./run-performance-tests.sh basic",
    "test:performance:load": "cd tests/performance && ./run-performance-tests.sh load",
    "test:performance:stress": "cd tests/performance && ./run-performance-tests.sh stress",
